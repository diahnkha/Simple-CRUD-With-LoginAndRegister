@model RAS.Bootcamp.Mvc.Net.Models.Entities.Pembeli;


<form asp-controller="Pembeli" asp-action="Edit" method="post">
    <input asp-for="Id" hidden />
    <div class="mb-3">
        <label class="form-label">Id</label>
        <input asp-for=Id type="number" disabled class="form-control" />
    </div>

    <div class="mb-3">
        <label class="form-label">ID User</label>
        <input asp-for=IdUser type="number" class="form-control" />
    </div>

    <div class="mb-3">
        <label class="form-label">Alamat</label>
        <input asp-for=Alamat type="text" class="form-control" />
    </div>

    <div class="mb-3">
        <label class="form-label">No HP</label>
        <input asp-for=NoHp type="text" class="form-control" />
    </div>

    <div class="mb-3">
        <button type="submit" class="btn btn-primary">Update</button>
    </div>

</form>

